查询全部
SELECT * FROM reet_tbl

查询一段日期全部
SELECT * FROM reet_tbl WHERE TIME BETWEEN '2017-11-14 15:00:00' AND '2017-11-14 17:00:00'

查询一段日期全部出现的某些点数 3-6
SELECT * FROM reet_tbl WHERE TIME BETWEEN '2017-11-14 15:00:00' AND '2017-11-14 17:00:00' AND (ZHUANG1 in (3,6) OR ZHUANG2 in (3,6) OR ZHUANG3 in (3,6)
OR XIAN1 in (3,6) OR XIAN2 in (3,6) OR XIAN3 in (3,6) OR ZHUANGVALUE in (3,6) OR XIANVALUE in (3,6))

查询一段日期庄出现的某些点数 3-6
SELECT * FROM reet_tbl WHERE TIME BETWEEN '2017-11-14 15:00:00' AND '2017-11-14 17:00:00' AND (ZHUANG1 in (3,6) OR ZHUANG2 in (3,6) OR ZHUANG3 in (3,6) OR ZHUANGVALUE in (3,6))

查询一段日期闲出现的某些点数 3-6
SELECT * FROM reet_tbl WHERE TIME BETWEEN '2017-11-14 15:00:00' AND '2017-11-14 17:00:00' AND (XIAN1 in (3,6) OR XIAN2 in (3,6) OR XIAN3 in (3,6) OR XIANVALUE in (3,6))

查询一段日期闲出现的某些点数 3-6 并且是闲是双数
SELECT * FROM reet_tbl WHERE TIME BETWEEN '2017-11-14 15:00:00' AND '2017-11-14 17:00:00' AND (XIAN1 in (3,6) OR XIAN2 in (3,6) OR XIAN3 in (3,6) OR XIANVALUE in (3,6))
AND (XIAN1%2=0 OR XIAN2%2=0 OR XIAN3%2=0 OR XIANVALUE%2=0)


SELECT * FROM reet_tbl WHERE MATCH (XIAN1,XIAN2,XIAN3) AGAINST (3,2,10);

此段查询语句返回 reet_tbl 表中 ROOMID 的重复次数（count）排名前 100 的数值。通过添加 DISTINCT 返回唯一记录。
# 可用于查询连庄、连闲次数
SELECT DISTINCT ROOMID, count( * ) AS count
FROM reet_tbl WHERE ZHUANGVALUE > XIANVALUE
GROUP BY ROOMID
ORDER BY count DESC
LIMIT 100

update reet_tbl set VALUE='A' where VALUE = '庄'
update reet_tbl set VALUE='B' where VALUE = '闲'
update reet_tbl set VALUE='C' where VALUE = '和'

SELECT
	JISHUCOUNT AS '单',
	OUSHUCOUNT AS '双',
	`VALUE` AS '结果',
	ROOMID,
	POINT
FROM
	reet_tbl
ORDER BY
	ROOMID,POINT ASC

#随机取5条数据
SELECT * FROM reet_tbl where XIAN1=7 ORDER BY RAND() LIMIT 5;

#导出庄强>7
SELECT
	ZHUANG1 AS '庄1',
	XIAN1 AS '闲1',
	ZHUANG2 AS '庄2',
	XIAN2 AS '闲2',
	ZHUANG3 AS '庄3',
	XIAN3 AS '闲3',
    ZHUANGVALUE AS '庄点数',
    XIANVALUE AS '闲点数',
	JISHUCOUNT AS '单',
	OUSHUCOUNT AS '双',
	`VALUE` AS '结果',
	ROOMID,
	POINT
FROM
	reet_tbl
WHERE
	TRENT = 1
ORDER BY
	ROOMID,
	POINT ASC


# 查询牌的数量倒序
SELECT
	*, count(PAI) AS PAICOUNT
FROM
	(
		SELECT
			*,
			GROUP_CONCAT(
				ZHUANG1,
				":",
				XIAN1,
				":",
				ZHUANG2,
				":",
				XIAN2,
				":",
				ZHUANG3,
				":",
				XIAN3
			) AS PAI
		FROM
			reet_tbl
		WHERE
			JISHUCOUNT = 3
		AND OUSHUCOUNT = 0
		GROUP BY
			ID
	) as TMP
GROUP BY
	PAI DESC ORDER BY PAICOUNT DESC;

CREATE TABLE `reet_analyze_tbl` (
  `ID` varchar(64) NOT NULL,
  `ROOMID` varchar(64) NOT NULL COMMENT '预测的大局数',
  `POINT` int(3) NOT NULL COMMENT '第几局',
  `MAXMINRESULTA` int(5) NOT NULL COMMENT '大小值预测庄个数',
  `MAXMINRESULTB` int(5) NOT NULL COMMENT '大小值预测闲个数',
  `MAXMINRESULTC` int(5) NOT NULL COMMENT '大小值预测和个数',
  `MAXMINRESULTTYPE` varchar(1) NOT NULL COMMENT '大小值预测查询类型A表示4组，B表示三组',
  `OLDEVENRESULTA` int(5) NOT NULL COMMENT '奇偶值预测庄个数',
  `OLDEVENRESULTB` int(5) NOT NULL COMMENT '奇偶值预测闲个数',
  `OLDEVENRESULTC` int(5) NOT NULL COMMENT '奇偶值预测和个数',
  `OLDEVENRESULTTYPE` varchar(1) NOT NULL COMMENT '奇偶值预测查询类型A表示4组，B表示三组,C表示2组',
  `MAXMINRESULTAVALUE` varchar(4) NOT NULL COMMENT '大小值预测结果',
  `OLDEVENRESULTAVALUE` varchar(4) NOT NULL COMMENT '奇偶值预测结果',
  `VALUE` varchar(1) NOT NULL COMMENT '实际结果',
  `MAXMINRESULTVALUE` varchar(1) NOT NULL COMMENT '大小值预测是否正确A表示准确，B表示错误',
  `OLDEVENRESULTVALUE` varchar(1) NOT NULL COMMENT '奇偶值预测是否正确A表示准确，B表示错误',
  `MNLSRESULTA` int(5) NOT NULL COMMENT '奇偶大小预测庄个数',
  `MNLSRESULTB` int(5) NOT NULL COMMENT '奇偶大小预测闲个数',
  `MNLSRESULTC` int(5) NOT NULL COMMENT '奇偶大小预测和个数',
  `MNLSRESULTTYPE` varchar(1) NOT NULL COMMENT '奇偶大小预测查询类型A表示4组，B表示三组',
  `MNLSRESULTAVALUE` varchar(4) NOT NULL COMMENT '奇偶大小预测结果',
  `MNLSRESULTVALUE` varchar(1) NOT NULL COMMENT '奇偶大小预测是否正确A表示准确，B表示错误',
  `MNRESULTA` int(5) NOT NULL COMMENT '奇偶预测庄个数',
  `MNRESULTB` int(5) NOT NULL COMMENT '奇偶预测闲个数',
  `MNRESULTC` int(5) NOT NULL COMMENT '奇偶预测和个数',
  `MNRESULTTYPE` varchar(1) NOT NULL COMMENT '奇偶预测查询类型A表示4组，B表示三组',
  `MNRESULTAVALUE` varchar(4) NOT NULL COMMENT '奇偶预测结果',
  `MNRESULTVALUE` varchar(1) NOT NULL COMMENT '奇偶预测是否正确A表示准确，B表示错误',
  `LSRESULTA` int(5) NOT NULL COMMENT '大小预测庄个数',
  `LSRESULTB` int(5) NOT NULL COMMENT '大小预测闲个数',
  `LSRESULTC` int(5) NOT NULL COMMENT '大小预测和个数',
  `LSRESULTTYPE` varchar(1) NOT NULL COMMENT '大小预测查询类型A表示4组，B表示三组',
  `LSRESULTAVALUE` varchar(4) NOT NULL COMMENT '大小预测结果',
  `LSRESULTVALUE` varchar(1) NOT NULL COMMENT '大小预测是否正确A表示准确，B表示错误',
  `ABRESULTA` int(5) NOT NULL COMMENT '庄闲值预测庄个数',
  `ABRESULTB` int(5) NOT NULL COMMENT '庄闲值预测闲个数',
  `ABRESULTC` int(5) NOT NULL COMMENT '庄闲值预测和个数',
  `ABRESULTTYPE` varchar(1) NOT NULL COMMENT '庄闲值预测查询类型A表示4组，B表示三组',
  `ABRESULTAVALUE` varchar(4) NOT NULL COMMENT '庄闲值预测结果',
  `ABRESULTVALUE` varchar(1) NOT NULL COMMENT '庄闲值预测是否正确A表示准确，B表示错误,C表示未知',
  `TIME` datetime DEFAULT NULL COMMENT '时间',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

#根据连续3局条件查询第四局结果
SELECT * FROM reet_tbl a INNER JOIN ( SELECT * FROM reet_tbl b INNER JOIN ( SELECT ROOMID AS dROOMID, POINT AS dPOINT, `VALUE` AS dVALUE, JISHUCOUNT AS dJISHUCOUNT, OUSHUCOUNT AS dOUSHUCOUNT FROM reet_tbl c INNER JOIN ( SELECT ROOMID AS dROOMID1, POINT AS dPOINT1, `VALUE` AS dVALUE, JISHUCOUNT AS dJISHUCOUNT1, OUSHUCOUNT AS dOUSHUCOUNT1 FROM reet_tbl f WHERE f.JISHUCOUNT = ? and f.OUSHUCOUNT = ? and f.VALUE = ? ) g ON c.ROOMID = g.dROOMID1 AND c.POINT = g.dPOINT1 + 1 WHERE c.JISHUCOUNT = ? and c.OUSHUCOUNT = ? and c.VALUE = ? ) d ON b.ROOMID = d.dROOMID AND b.POINT = d.dPOINT + 1 WHERE b.JISHUCOUNT = ? and b.OUSHUCOUNT = ? and b.VALUE = ? ) e ON a.POINT = e.POINT + 1 AND a.ROOMID = e.ROOMID ORDER BY a.TIME ASC LIMIT ?,?
